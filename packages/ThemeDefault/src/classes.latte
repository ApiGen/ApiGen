{layout '@layout.latte'}

{block title}{$pageTitle}{/block}

{block content}
    <h1>{$pageTitle}</h1>

    <table class="summary table table-bordered table-responsive table-striped">
        {foreach $classes as $class}
            <tr>
                <td class="name">
                    <a href="{$class|linkReflection}" n:class="$class->isDeprecated() ? deprecated">{$class->getName()}</a>
                </td>
            </tr>
        {/foreach}
    </table>
{/block}
