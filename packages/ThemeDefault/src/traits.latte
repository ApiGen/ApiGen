{layout '@layout.latte'}

{block title}{$pageTitle}{/block}

{block content}
    <h1>{$pageTitle}</h1>

    <table class="summary table table-bordered table-responsive table-striped">
        {foreach $traits as $trait}
            <tr>
                <td class="name">
                    <a href="{$trait|linkReflection}" n:class="$trait->isDeprecated() ? deprecated">{$trait->getName()}</a>
                </td>
            </tr>
        {/foreach}
    </table>
{/block}
