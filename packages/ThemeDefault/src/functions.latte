{layout '@layout.latte'}

{block title}{$pageTitle}{/block}

{block content}
    <h1>{$pageTitle}</h1>

    <table class="summary table table-bordered table-responsive table-striped">
        {foreach $functions as $function}
            <tr>
                <td class="name">
                    <a href="{$function|linkReflection}" n:class="$function->isDeprecated() ? deprecated">{$function->getName()}</a>
                </td>
            </tr>
        {/foreach}
    </table>
{/block}
